120,"column":0},"source":"pages/signup.js","original":{"line":98,"column":1}},{"generated":{"line":120,"column":1},"source":"pages/signup.js","original":{"line":32,"column":0}},{"name":"SignUp","generated":{"line":122,"column":15},"source":"pages/signup.js","original":{"line":100,"column":15}}],"sources":{"pages/signup.js":"import React, { useEffect, useState } from 'react';\nimport styled from 'styled-components';\nimport { useMutation, useApolloClient, gql } from '@apollo/client';\n\nimport Button from '../components/Button';\n\nconst SIGNUP_USER = gql`\n    mutation SignUp($email: String!, $username: String!, $password: String!) {\n        signUp(email: $email, username: $username, password: $password)\n    }\n`;\n\nconst Wrapper = styled.div`\n    border: 1px solid #f5f4f0;\n    max-width: 500px;\n    padding: 1em;\n    margin: 0 auto;\n`;\n\nconst Form = styled.form`\n    label,\n    input {\n        display: block;\n        line-height: 2em;\n    }\n    input {\n        width: 100%;\n        margin-bottom: 1em;\n    }\n`;\n\nconst SignUp = props => {\n    const [values, setValues] = useState();\n\n    const [SignUp, { loading, error }] = useMutation(SIGNUP_USER, {\n        onCompleted: data => {\n            console.log('SignUp completed?');\n            console.log(data.signUp);\n        }\n    });\n\n    const onChange = event => {\n        setValues({\n            ...values,\n            [event.target.name]: event.target.value\n        });\n    };\n\n    useEffect( () => {\n        document.title = 'Sign Up - Notedly';\n    });\n\n    return (\n        <Wrapper>\n            <h2>Sign Up</h2>\n            <Form\n                onSubmit={event => {\n                    event.preventDefault();\n                    SignUp({\n                        variables: {\n                            ...values\n                        }\n                    });\n                    // console.log(values);\n                }}\n            >\n                <label htmlFor=\"username\">Username:</label>\n                <input\n                    required\n                    type=\"text\"\n                    id=\"username\"\n                    name=\"username\"\n                    placeholder=\"Username\"\n                    onChange={onChange}\n                />\n                <label htmlFor=\"email\">Email:</label>\n                <input\n                    required\n                    type=\"email\"\n                    id=\"email\"\n                    name=\"email\"\n                    placeholder=\"Email\"\n                    onChange={onChange}\n                />\n                <label htmlFor=\"password\">Password:</label>\n                <input\n                    required\n                    type=\"password\"\n                    id=\"password\"\n                    name=\"password\"\n                    placeholder=\"Password\"\n                    onChange={onChange}\n                />\n                <Button type=\"submit\">Submit</Button>\n            </Form>\n        </Wrapper>\n    );\n};\n\nexport default SignUp;"},"lineCount":null}},"error":null,"hash":"deaba6871567551dbbd0d406604dff68","cacheData":{"env":{}}}